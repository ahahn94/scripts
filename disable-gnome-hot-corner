# This program installs the gnome-extension "no topleft hot corner"
#

#!/bin/bash
cd ~
git clone https://github.com/HROMANO/nohotcorner.git
mv nohotcorner nohotcorner@azuri.free.fr
mkdir -p .local/share/gnome-shell/extensions
mv nohotcorner@azuri.free.fr .local/share/gnome-shell/extensions/
